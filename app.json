{
  "expo": {
    "name": "Vroom",
    "scheme":"Vroom",
    "slug": "Vroom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "updates": {
        "enabled": true,
        "fallbackToCacheTimeout": 0
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vroomde.Vroom"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vroomde.Vroom",
      "permissions": ["CAMERA", "LOCATION"]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZGF0YWV4cGxvcmVycyIsImEiOiJjbG1tbmlqNGYwN2MyMnFtcDl0OGp5bGh5In0.ov0__pXbmfC6op7cyZW7Pg"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3f81d3ef-7e51-45cd-8666-aee1b88436ac"
      }
    }
  }
}
